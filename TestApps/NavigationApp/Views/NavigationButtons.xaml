<UserControl xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mui="https://github.com/JohanLarsson/Gu.Wpf.ModernUI">
    <StackPanel>
        <Button AutomationProperties.AutomationId="GoToPageButton"
                Command="GoToPage"
                CommandParameter="/Views/View1.xaml"
                Content="Goto 1" />

        <mui:Link AutomationProperties.AutomationId="GoToPageLink"
                  Command="GoToPage"
                  CommandParameter="/Views/View1.xaml"
                  DisplayName="Goto 1" />

        <mui:Link AutomationProperties.AutomationId="SourceLink"
                  Source="/Views/View1.xaml"
                  DisplayName="Goto 1" />
    </StackPanel>
</UserControl>
