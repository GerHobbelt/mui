<UserControl xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mui="https://github.com/JohanLarsson/Gu.Wpf.ModernUI">
    <StackPanel>
        <Button x:Name="GoToPageButton"
                Command="GoToPage"
                CommandParameter="/View1.xaml"
                Content="Goto 1" />

        <mui:Link x:Name="GoToPageLink"
                  Command="GoToPage"
                  CommandParameter="/View1.xaml"
                  DisplayName="Goto 1" />

        <mui:Link x:Name="SourceLink"
                  Source="/View1.xaml"
                  DisplayName="Goto 1" />
    </StackPanel>
</UserControl>
