<UserControl x:Class="Gu.ModernUI.Demo.Pages.Settings"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:controls="clr-namespace:Gu.ModernUI.Windows.Controls;assembly=Gu.ModernUI"
             xmlns:presentation="clr-namespace:Gu.ModernUI.Presentation;assembly=Gu.ModernUI"
             mc:Ignorable="d" >
    <Grid Style="{StaticResource ContentRoot}">
        <controls:ModernTab SelectedSource="/Content/SettingsAppearance.xaml" Layout="List">
            <controls:ModernTab.Links>
                <presentation:Link DisplayName="Appearance" Source="/Content/SettingsAppearance.xaml" />
            </controls:ModernTab.Links>
        </controls:ModernTab>
    </Grid>
</UserControl>
